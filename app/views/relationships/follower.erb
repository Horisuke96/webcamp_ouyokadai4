<h2>Follow Users</h2>
<tr>
 <% if @user.exists? %>
  <th>image</th>
  <th>name</th>
  <th colspan="3"></th>
</tr>
<tbody>
 <% @users.each do |user| %>
 <tr>
  <td><%= attachment_image_tag(user, :profile_image, :fill, 50, 50, fallback: "no-image-icon.jpg") %></td>     
  <td><%= user.name %></td>
  <td>フォロー数: <%= user.followings.count %></td>
  <td>フォロワー数: <%= user.followers.count %></td>
  <td><%= link_to "Show", user %></td>
 <tr>
 <% end %>
</tbody>
 <% else %>
  <th>ユーザーはいません</th>
<% end %>